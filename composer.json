{
    "name": "speedwork/database",
    "type": "library",
    "description": "Speedwork database Library",
    "license": "MIT",
    "authors": [{
        "name": "sankar",
        "email": "sankar.suda@gmail.com"
    }],
    "require": {
        "php": ">=5.6",
        "doctrine/dbal" : "^2.5"
    },
    "replace" : {
        "speedwork/container": "self.version",
        "speedwork/core": "self.version",
        "speedwork/util": "self.version"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5",
        "satooshi/php-coveralls": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Speedwork\\Database\\": "src"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
